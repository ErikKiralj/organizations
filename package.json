{
  "name": "organizations",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "yarn lint:cspell && yarn lint:eslint && yarn lint:stylelint && yarn lint:prettier && yarn lint:package-json",
    "lint:cspell": "cspell '**' --no-progress --no-summary",
    "lint:eslint": "eslint './**/*.{js,ts,tsx}' --quiet --cache --cache-strategy content --cache-location '.eslintcache/'",
    "lint:package-json": "npmPkgJsonLint --configFile ./.packagerc.json .",
    "lint:prettier": "prettier --check \"./**/*{json,md,css}\"",
    "lint:stylelint": "stylelint --cache './**/*.css'",
    "start": "next start"
  },
  "dependencies": {
    "next": "13.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "@types/react": "^18.2.29",
    "@types/react-dom": "^18.2.13",
    "@types/react-test-renderer": "^18.0.4",
    "@typescript-eslint/parser": "^5.14.0",
    "cspell": "^7.3.8",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-package-json-lint": "^7.0.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.1.6"
  }
}
